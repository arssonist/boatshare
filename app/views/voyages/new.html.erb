<div class="voyage-new-field">



<%= form_for [@voyage] do |f| %>

  <div>
   <%= f.label :title %>
   <%= f.text_field :title %>
   </div>

  <div>
    <%= f.label :description %>
    <%= f.text_area :description%>
  </div>

  <div>
    <%= f.label :location %>
    <%= f.text_field :location %>
  </div>

  <div>
    <%= f.label :start_time %>
    <%= f.datetime_field :start_time %>
  </div>

  <div>
    <%= f.label :end_time %>
    <%= f.datetime_field :end_time %>
  </div>



    <%= f.submit %>
      <% end %>

</div>
